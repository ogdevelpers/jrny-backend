{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "Portfolio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "key": {
      "type": "uid",
      "targetField": "Project_Name"
    },
    "Project_Year": {
      "type": "string"
    },
    "Project_Heading": {
      "type": "string"
    },
    "Project_Name": {
      "type": "string"
    },
    "Project_Description": {
      "type": "text"
    },
    "Project_Video_Url": {
      "type": "string"
    },
    "Project_Sub_Heading": {
      "type": "text"
    },
    "Project_Sub_Description": {
      "type": "text"
    },
    "CTA_Text": {
      "type": "string"
    },
    "CTA_Link": {
      "type": "string"
    },
    "Project_Sub_Heading_2": {
      "type": "text"
    },
    "Location": {
      "type": "enumeration",
      "enum": [
        "Mumbai",
        "Delhi"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::portfolio-category.portfolio-category",
      "inversedBy": "portfolios"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "customField",
      "options": {
        "preset": "defaultHtml"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
